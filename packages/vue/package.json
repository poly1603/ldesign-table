  },
  "keywords": [
    "table",
    "vue",
    "vue3"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@ldesign/table-core": "workspace:*",
    "@ldesign/table-styles": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.4",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0"
  }
}


