{
  "name": "@ldesign/table-vue",
  "version": "0.1.0",
  "description": "Vue 3 组件封装 for @ldesign/table-core",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --emitDeclarationOnly --declaration --outDir dist",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "@ldesign/table-core": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vue": "^3.3.8",
    "vue-tsc": "^1.8.22"
  },
  "keywords": [
    "vue",
    "vue3",
    "table",
    "grid"
  ],
  "license": "MIT"
}
