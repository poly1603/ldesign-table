{
  "name": "@ldesign/table-lit",
  "version": "0.1.0",
  "description": "Lit Web Components表格组件",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf dist"
  },
  "keywords": [
    "table",
    "lit",
    "web-components"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ldesign/table-core": "workspace:*",
    "@ldesign/table-styles": "workspace:*",
    "lit": "^3.1.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.4"
  }
}


