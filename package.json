{
  "name": "@ldesign/table-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "高性能框架无关表格插件",
  "scripts": {
    "dev": "pnpm --filter \"./packages/**\" dev",
    "build": "pnpm --filter \"./packages/**\" build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "clean": "pnpm --filter \"./packages/**\" clean && rimraf node_modules",
    "benchmark": "tsx scripts/benchmark.ts",
    "generate-data": "tsx scripts/generate-demo-data.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "table",
    "grid",
    "virtual-scroll",
    "vue",
    "react",
    "lit",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
